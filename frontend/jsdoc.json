{
    "source": {
        "include": ["src"],
        "includePattern": ".+\\.jsx?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/jsdoc",
        "recurse": true,
        "verbose": true
    },
    "plugins": [
        "plugins/markdown"
    ]
}

